<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.lang.String?>
<StackPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.submittedConfessionController">
   <children>
      <AnchorPane onMousePressed="#click" prefHeight="513.0" prefWidth="420.0" stylesheets="@styles.css">
         <children>
            <VBox alignment="TOP_CENTER" layoutX="63.0" layoutY="8.0" prefHeight="458.0" prefWidth="380.0" spacing="20.0" style="-fx-background-color: white;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="85.0" prefWidth="380.0" style="-fx-background-color: bisque;" styleClass="reactions-container" stylesheets="@styles.css">
                     <children>
                        <ImageView fitHeight="68.0" fitWidth="72.0">
                           <image>
                              <Image url="@icon/confesslogo.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="mainLabel" prefHeight="50.0" prefWidth="206.0">
                           <font>
                              <Font name="Century Gothic Bold" size="14.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="20.0" right="20.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" top="20.0" />
                     </VBox.margin>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </HBox>
                  <Label fx:id="submissionLabel" alignment="TOP_LEFT" prefHeight="288.0" prefWidth="350.0" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Cambria" size="15.0" />
                     </font>
                  </Label>
                  <Button mnemonicParsing="false" onAction="#thankYouButtonClicked" prefHeight="39.0" prefWidth="94.0" style="-fx-background-radius: 30;">
                     <graphic>
                        <ImageView fitHeight="26.0" fitWidth="26.0">
                           <image>
                              <Image url="@icon/confirm.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding></Button>
               </children>
            </VBox>
         </children>
         <styleClass>
            <String fx:value="round-button" />
            <String fx:value="shadow" />
         </styleClass>
      </AnchorPane>
   </children>
</StackPane>
