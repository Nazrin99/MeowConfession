                <HBox alignment="CENTER" prefHeight="88.0" prefWidth="663.0" style="-fx-background-color: #072f5f;" VBox.vgrow="SOMETIMES">
                    <children>
                        <Label fx:id="searchLabel" alignment="CENTER" prefHeight="31.0" prefWidth="535.0" textAlignment="CENTER" textFill="WHITE">
                            <font>
                                <Font name="Book Antiqua Bold" size="20.0" />
                            </font>
                        </Label>
                    </children>
                </HBox>
            <HBox alignment="CENTER" prefHeight="77.0" prefWidth="663.0">
               <children>
                  <Label fx:id="searchResults" alignment="CENTER" prefHeight="36.0" prefWidth="535.0" textAlignment="CENTER">
