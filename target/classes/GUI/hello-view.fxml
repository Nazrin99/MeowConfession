<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>


<Pane fx:id="originalPane" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.HelloController">
   <children>
      <StackPane fx:id="firstStackPane" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Button fx:id="firstButton" mnemonicParsing="false" onAction="#firstButtonClicked" text="FirstStack" />
         </children>
      </StackPane>
      <StackPane fx:id="secondStackPane" prefHeight="150.0" prefWidth="200.0">
         <children>
            <Button fx:id="secondButton" mnemonicParsing="false" onAction="#secondButtonClicked" text="SecondStack" />
         </children>
      </StackPane>
   </children>
</Pane>
